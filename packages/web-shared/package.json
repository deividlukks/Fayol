{
  "name": "@fayol/web-shared",
  "version": "0.1.1",
  "description": "Shared contexts, hooks and utilities for Fayol web applications",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist .turbo node_modules",
    "lint": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "catalog:",
    "react-dom": "catalog:",
    "next": "catalog:"
  },
  "dependencies": {
    "@fayol/api-client": "workspace:*",
    "@fayol/shared-types": "workspace:*",
    "axios": "catalog:"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "eslint": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "rimraf": "catalog:"
  }
}
