{
  "name": "@fayol/api-client-core",
  "version": "0.1.1",
  "description": "Fayol API Client Core - Shared logic for web and mobile clients",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./storage": {
      "types": "./dist/storage.interface.d.ts",
      "import": "./dist/storage.interface.js",
      "default": "./dist/storage.interface.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist .turbo node_modules",
    "lint": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "axios": "catalog:",
    "@fayol/shared-types": "workspace:*",
    "@fayol/shared-constants": "workspace:*",
    "@fayol/validation-schemas": "workspace:*"
  },
  "peerDependencies": {
    "@fayol/shared-types": "workspace:*",
    "@fayol/shared-constants": "workspace:*",
    "@fayol/validation-schemas": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "eslint": "catalog:",
    "rimraf": "catalog:"
  }
}
