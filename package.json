{
  "name": "fayol",
  "version": "0.1.1",
  "description": "Sistema Multiplataforma de GestÃ£o Financeira Pessoal com IA",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deividlukks/fayol.git"
  },
  "scripts": {
    "dev": "pnpm run validate-env && turbo run dev --parallel",
    "dev:web": "pnpm run validate-env && turbo run dev --filter=web-app",
    "dev:admin": "pnpm run validate-env && turbo run dev --filter=admin-panel",
    "dev:both": "pnpm run validate-env && turbo run dev --filter=web-app --filter=admin-panel --parallel",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=web-app",
    "build:admin": "turbo run build --filter=admin-panel",
    "//postinstall": "pnpm --filter @fayol/database-models run generate",
    "generate": "turbo run generate",
    "test": "turbo run test",
    "test:unit": "turbo run test",
    "test:integration": "turbo run test:e2e",
    "test:watch": "turbo run test:watch",
    "test:coverage": "turbo run test:coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "turbo run test:e2e",
    "test:all": "pnpm run test:unit && pnpm run test:integration",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "type-check": "turbo run type-check",
    "start": "pnpm run validate-env && turbo run start",
    "clean": "turbo run clean && rimraf node_modules",
    "clean:workspaces": "turbo run clean",
    "setup": "pnpm install && pnpm run prepare",
    "prepare": "husky",
    "validate-env": "node scripts/validate-env.js",
    "validate-env:test": "NODE_ENV=test node scripts/validate-env.js",
    "validate-env:prod": "NODE_ENV=production node scripts/validate-env.js",
    "docker:manager": "bash scripts/docker-compose-manager.sh || cmd.exe /c scripts\\docker-compose-manager.bat",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:status": "docker-compose ps",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v",
    "docker:validate": "bash scripts/docker-compose-manager.sh validate || cmd.exe /c scripts\\docker-compose-manager.bat validate",
    "db:generate": "pnpm --filter @fayol/database-models run generate",
    "db:migrate": "pnpm --filter @fayol/database-models run migrate:dev",
    "db:migrate:deploy": "pnpm --filter @fayol/database-models run migrate:deploy",
    "db:seed": "pnpm --filter @fayol/database-models run seed",
    "db:studio": "pnpm --filter @fayol/database-models run studio",
    "db:reset": "pnpm --filter @fayol/database-models run migrate:dev -- --name reset",
    "sync-catalog": "node scripts/sync/sync-catalog.js",
    "sync-catalog:fix": "node scripts/sync/sync-catalog.js --fix",
    "sync-catalog:add": "node scripts/sync/sync-catalog.js --add",
    "validate-catalog": "node scripts/validation/validate-catalog.js",
    "env:start": "node scripts/environment/start.js",
    "env:start:fast": "node scripts/environment/start.js --fast",
    "env:setup": "node scripts/environment/setup-vault-and-backup.js",
    "vault:init": "node scripts/vault/init-vault.js",
    "backup:create": "node scripts/backup/backup-postgres.js",
    "backup:restore": "node scripts/backup/restore-postgres.js",
    "backup:list": "node scripts/backup/list-backups.js",
    "prisma:debug": "node scripts/prisma/debug-queries.js",
    "prisma:analyze": "node scripts/prisma/analyze-queries.js",
    "prisma:audit": "node scripts/prisma/audit-performance.js",
    "db:test": "node scripts/database/test-db-connection.js"
  },
  "packageManager": "pnpm@10.26.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "catalog:",
    "@commitlint/config-conventional": "catalog:",
    "@playwright/test": "catalog:",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/user-event": "catalog:",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "dotenv-cli": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "husky": "catalog:",
    "identity-obj-proxy": "catalog:",
    "jest": "catalog:",
    "jest-environment-jsdom": "catalog:",
    "js-yaml": "catalog:",
    "lint-staged": "catalog:",
    "prettier": "catalog:",
    "rimraf": "catalog:",
    "ts-jest": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@nestjs/config": "catalog:",
    "@opentelemetry/instrumentation": "catalog:",
    "@opentelemetry/instrumentation-express": "catalog:",
    "@opentelemetry/instrumentation-http": "catalog:",
    "@opentelemetry/sdk-trace-base": "catalog:",
    "@opentelemetry/sdk-trace-node": "catalog:",
    "@sentry/node": "catalog:",
    "prom-client": "catalog:",
    "winston": "catalog:"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^18.2.57",
      "@types/react-dom": "^18.2.19",
      "@opentelemetry/sdk-trace-base": "^1.28.0",
      "next@>=13.4.0 <14.1.1": ">=14.1.1",
      "next@>=14.0.0 <14.2.10": ">=14.2.10",
      "next@>=10.0.0 <14.2.7": ">=14.2.7",
      "next@>=14.0.0 <14.2.21": ">=14.2.21",
      "next@>=13.0 <14.2.30": ">=14.2.30",
      "next@>=0.9.9 <14.2.31": ">=14.2.31",
      "next@>=9.5.5 <14.2.15": ">=14.2.15",
      "next@>=0.9.9 <14.2.32": ">=14.2.32",
      "next@>=0.9.9 <14.2.24": ">=14.2.24",
      "next@>=14.0.0 <14.2.25": ">=14.2.25",
      "tmp@<=0.2.3": ">=0.2.4",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "glob@>=10.2.0 <10.5.0": ">=10.5.0",
      "next@>=13.3.0 <14.2.34": ">=14.2.34",
      "next@>=13.3.1-canary.0 <14.2.35": ">=14.2.35"
    },
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@prisma/engines",
      "@scarf/scarf",
      "@sentry/profiling-node",
      "@whiskeysockets/baileys",
      "esbuild",
      "msgpackr-extract",
      "prisma",
      "protobufjs",
      "sharp",
      "tesseract.js",
      "unrs-resolver"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
